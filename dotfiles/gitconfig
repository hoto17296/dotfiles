[alias]
  st = status -s -b
  cm = commit -a
  df = diff --color-words
  br = branch
  co = checkout
  cb = checkout -b
  cl = clone
  ft = fetch
  ps = push
  pl = pull
  rb = rebase
  gr = grep
  ss = stash
  lg = log --stat
  amn = commit -a --amend --no-edit
  tree = log --graph --decorate --oneline
  hist = log -p
  recent-branch = for-each-ref --format='%(refname:short)' --sort=-committerdate refs/heads
  current-branch = rev-parse --abbrev-ref HEAD
[diff]
  renames = true
[fetch]
  prune = true
[core]
  excludesfile = ~/.gitignore
[include]
  path = ~/.gitconfig.local

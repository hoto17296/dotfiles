[alias]
  st = status -s -b
  cm = commit -a
  df = diff --color-words
  br = branch
  co = checkout
  cb = checkout -b
  cl = clone
  ft = fetch
  ps = push
  pl = pull
  rb = rebase
  gr = grep
  ss = stash
  lg = log --stat
  tree = log --graph --decorate --oneline
  hist = log -p
[diff]
  renames = true
[core]
  excludesfile = ~/.gitignore
[include]
  path = ~/.gitconfig.local

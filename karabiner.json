{
    "global": {
        "check_for_updates_on_startup": true,
        "show_in_menu_bar": true,
        "show_profile_name_in_menu_bar": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.simultaneous_threshold_milliseconds": 50,
                    "basic.to_delayed_action_delay_milliseconds": 500,
                    "basic.to_if_alone_timeout_milliseconds": 1000,
                    "basic.to_if_held_down_threshold_milliseconds": 500,
                    "mouse_motion_to_scroll.speed": 100
                },
                "rules": [
                    {
                        "description": "左右 command 単押しで英かな切り替え",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_command",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_command",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_eisuu"
                                    }
                                ],
                                "to_if_held_down": [
                                    {
                                        "key_code": "left_command"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_command",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "right_command",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_kana"
                                    }
                                ],
                                "to_if_held_down": [
                                    {
                                        "key_code": "right_command"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Caps Lock 単押しで Esc, 複合で Ctrl",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_control"
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "escape"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Del 長押しで 1Password クイックアクセスを開く",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "delete_forward"
                                },
                                "to_if_held_down": {
                                    "key_code": "backslash",
                                    "modifiers": [
                                        "right_command",
                                        "right_option"
                                    ],
                                    "repeat": false
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "command + fn + 1 でミラーリング設定を切り替える",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "1",
                                    "modifiers": {
                                        "mandatory": [
                                            "command",
                                            "fn"
                                        ]
                                    }
                                },
                                "to": {
                                    "key_code": "display_brightness_decrement",
                                    "modifiers": [
                                        "command"
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "option + [ で内臓ディスプレイの輝度を下げる",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "open_bracket",
                                    "modifiers": {
                                        "mandatory": [
                                            "option"
                                        ]
                                    }
                                },
                                "to": {
                                    "key_code": "display_brightness_decrement"
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "option + ] で内臓ディスプレイの輝度を上げる",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "close_bracket",
                                    "modifiers": {
                                        "mandatory": [
                                            "option"
                                        ]
                                    }
                                },
                                "to": {
                                    "key_code": "display_brightness_increment"
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "PgUp/PgDn でタブ移動",
                        "manipulators": [
                            {
                                "description": "VSCode と Vivaldi は macOS の不具合で [ を含むショートカットが届かないので特別対応",
                                "conditions": [
                                    {
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": [
                                            "^com\\.vivaldi\\.Vivaldi$",
                                            "^com\\.microsoft\\.VSCode$"
                                        ]
                                    }
                                ],
                                "from": {
                                    "key_code": "page_up"
                                },
                                "to": {
                                    "key_code": "left_arrow",
                                    "modifiers": [
                                        "right_command",
                                        "right_option"
                                    ]
                                },
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "page_up"
                                },
                                "to": {
                                    "key_code": "open_bracket",
                                    "modifiers": [
                                        "right_command",
                                        "right_shift"
                                    ]
                                },
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "page_down"
                                },
                                "to": {
                                    "key_code": "close_bracket",
                                    "modifiers": [
                                        "right_command",
                                        "right_shift"
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Vivaldi と VSCode に command + shift + [ が届かない問題の暫定対応",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": [
                                            "^com\\.vivaldi\\.Vivaldi$",
                                            "^com\\.microsoft\\.VSCode$"
                                        ]
                                    }
                                ],
                                "from": {
                                    "key_code": "open_bracket",
                                    "modifiers": {
                                        "mandatory": [
                                            "right_command",
                                            "right_shift"
                                        ]
                                    }
                                },
                                "to": {
                                    "key_code": "left_arrow",
                                    "modifiers": [
                                        "right_command",
                                        "right_option"
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "option + c で Visual Studio Code にフォーカスする",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "c",
                                    "modifiers": {
                                        "mandatory": [
                                            "option"
                                        ]
                                    }
                                },
                                "to": {
                                    "shell_command": "osascript -e 'tell application \"Code\" to activate'"
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "option + s で Slack にフォーカスする",
